                href="/contact">Book Tiffin<i class="ri-contacts-book-line"></i></a></div>
                <div id="linked"><i class="ri-align-justify"></i>
                    <div id="linkbox">
                        <a href="/" style="padding-top: 0vw;">Home<i class="ri-home-line"></i></a><a href="/about">About<i
                            class="ri-chat-2-line"></i></a><a href="/delivery">Delivery Details<i
                            class="ri-briefcase-5-line"></i></a><a href="/menu">Menu<i class="ri-menu-add-line"></i></a><a
                        href="/contact">Book Tiffin<i class="ri-contacts-book-line"></i></a>
                    </div>
                </div>
    </div>
    <div id="cntdiv"><h1 style="margin: 50px;  color: #4a3737;">Order Placed SuccessFully </h1>

        <div class="svg-container">    
            <svg class="ft-green-tick" xmlns="http://www.w3.org/2000/svg" height="100" width="100" viewBox="0 0 48 48" aria-hidden="true">
                <circle class="circle" fill="#5bb543" cx="24" cy="24" r="22"/>
                <path class="tick" fill="none" stroke="#FFF" stroke-width="6" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="10" d="M14 27l5.917 4.917L34 17"/>
            </svg>
        </div></div>
    <script src="../javascripts/script.js"></script>

    <script>
        let path = document.querySelector(".tick");
let length = path.getTotalLength();

console.log(length);

var navopen=0;

function openNav(){
  document.querySelector("#linked>i").addEventListener("click",function(){
    if(navopen===0){
      document.querySelector("#linkbox").style.display="flex";
      navopen=1;
    }
    else if(navopen===1){
      document.querySelector("#linkbox").style.display="none";
      navopen=0;
    }
  })
}
openNav();
    </script>
