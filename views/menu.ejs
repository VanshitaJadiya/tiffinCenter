                                    <ul>
                                        <li>Daal</li>
                                        <li>Chawal</li>
                                        <li>Bhindi fry</li>
                                        <li>Roti</li>
                                    </ul>
                                </div>
                            </div>
                            <div class="swiper-slide">
                                <div id="eve">
                                    <div class="img1"><img src="../images/pictures/pasta.jpg" alt=""></div>
                                    <div class="img2"><img src="../images/pictures/sandwich.jpg" alt=""></div>
