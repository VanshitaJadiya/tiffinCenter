                                    <ul>
                                        <li>Pasta</li>
                                        <h5>OR</h5>
                                        <li>SandWich</li>
                                    </ul>
                                </div>
                            </div>
                            <div class="swiper-slide">
                                <div id="dinner">
                                    <div class="img1"><img src="../images/pictures/daalchawal.jpg" alt=""></div>
                                    <div class="img2"><img src="../images/pictures/rajma.jpg" alt=""></div>
